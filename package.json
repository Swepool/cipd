  "keywords": [
    "cosmos",
    "wallet",
    "keplr",
    "leap",
    "cosmostation",
    "provider",
    "discovery",
    "eip-6963",
    "cipd"
  ],
  "author": "luknyb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Swepool/cipd.git"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
